   "source": [
    "# Exactitud promedio de la predicción <<<<<<------- ¿?\n",
    "accuracy = neigh.score(X_test, y_test)\n",
    "print 'E: ', accuracy"
    "i = 0\n",
    "yei = 0\n",
    "for i in range(len(X_predict)):\n",
